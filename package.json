  "scripts": {
    "dev": "nodemon",
    "build": "rimraf dist && tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "start": "node dist/index.js",
    "eslint": "eslint src",
    "eslint-fix": "eslint src --fix",
    "lint-fix": "prettier --write \"{src,tests,examples}/**/*.{ts,js}\"",
    "lint": "prettier --check \"{src,tests,examples}/**/*.{ts,js}\"",
    "check:type": "tsc --noEmit && tsc",
    "cli": "node -r dotenv/config -r tsconfig-paths/register -r ts-node/register",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha --timeout 10000 -r dotenv/config -r tsconfig-paths/register -r ts-node/register tests/**/*.test.ts",
    "testFile": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha --timeout 10000 -r dotenv/config -r tsconfig-paths/register -r ts-node/register ",
    "runFile": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha --timeout 10000 -r tsconfig-paths/register -r ts-node/register ",
    "clean-setup": "docker-compose down && docker volume rm be-starter_mongodb_data && docker volume rm be-starter_redis_data",
    "dc-up": "docker-compose up -d",
    "dc-down": "docker-compose down",
    "up-dev": "yarn run dc-up && yarn dev",
    "migrate-init": "migrate-mongo init",
    "migrate-add": "migrate-mongo create",
    "migrate-up": "dotenv migrate-mongo up",
    "migrate-down": "dotenv migrate-mongo down",
    "format": "prettier --write .",
    "rd-dev": "cross-env NODE_ENV=docker REDIS_DOCKER=redis://redis:6379 docker-compose up"
  },
